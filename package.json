{
  "name": "@atomist/org-visualizer",
  "version": "0.1.20",
  "description": "Organization Visualizer using Atomist project scanning",
  "author": "Atomist Inc.",
  "license": "Apache-2.0",
  "homepage": "https://github.com/atomist-blogs/org-visualizer#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomist-blogs/org-visualizer.git"
  },
  "bugs": {
    "url": "https://github.com/atomist-blogs/org-visualizer/issues"
  },
  "bin": {
    "spider": "./lib/script/spider.js"
  },
  "keywords": [
    "atomist",
    "automation",
    "sdm",
    "seed"
  ],
  "main": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@atomist/automation-client": "^1.6.2",
    "@atomist/automation-client-ext-humio": "^0.2.0",
    "@atomist/sdm": "^1.6.1",
    "@atomist/sdm-core": "^1.6.1",
    "@atomist/sdm-pack-analysis": "^1.1.2",
    "@atomist/sdm-pack-analysis-node": "^2.0.0",
    "@atomist/sdm-pack-analysis-spring": "2.0.0",
    "@atomist/sdm-pack-build": "^1.0.5",
    "@atomist/sdm-pack-docker": "^2.0.2",
    "@atomist/sdm-pack-fingerprints": "3.2.3-master.20190712041903",
    "@atomist/sdm-pack-node": "^1.1.1",
    "@atomist/sdm-pack-sloc": "1.0.3-master.20190502175705",
    "@atomist/sdm-pack-spring": "^2.0.0",
    "@atomist/slack-messages": "^1.1.1",
    "@atomist/uhura": "1.0.0-master.20190511162233",
    "@octokit/rest": "^16.28.4",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/yamljs": "^0.2.30",
    "app-root-path": "^2.2.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "es6-promise-pool": "^2.5.0",
    "express": "^4.17.1",
    "express-auth-parser": "^0.1.2",
    "express-handlebars": "^3.1.0",
    "fs-extra": "^8.1.0",
    "helmet": "^3.18.0",
    "lodash": "^4.17.14",
    "node-fetch": "^2.6.0",
    "pg": "^7.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "recursive-readdir": "^2.2.2",
    "serve-static": "^1.14.1",
    "stringify-tree": "^1.0.2",
    "yamljs": "^0.3.0",
    "yargs": "^13.3.0",
    "zip-a-folder": "0.0.8"
  },
  "devDependencies": {
    "@atomist/sdm-local": "^1.0.10",
    "@types/d3": "^5.7.2",
    "@types/mocha": "^5.2.7",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^9.0.2",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.3",
    "supervisor": "^0.12.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "run-s compile test lint doc",
    "clean": "run-p clean:compile clean:test clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"index.{d.ts,js{,.map}}\" \"{lib,test}/**/*.{d.ts,js{,.map}}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "clean:test": "rimraf .nyc_output coverage",
    "compile": "run-s git:info gql:gen compile:ts",
    "compile:ts": "tsc --project .",
    "doc": "typedoc --mode modules --excludeExternals --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "git:info": "atm-git-info",
    "gql:gen": "atm-gql-gen",
    "lint": "tslint --config tslint.json --format verbose --project .",
    "lint:fix": "npm run lint -- --fix",
    "start": "atm-start",
    "test": "mocha --require espower-typescript/guess \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\"",
    "typedoc": "npm run doc",
    "spider": "npm run compile:ts && node ./lib/script/spider.js -o Azure-Samples -s cognitive"
  },
  "engines": {
    "node": ">=8.1.0",
    "npm": ">=5.0.0"
  }
}
